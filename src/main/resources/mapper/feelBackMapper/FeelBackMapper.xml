<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.join.GoodsTradingProgram.mapper.feelBackMapper.FeelBackMapper">
    <resultMap id="goods" type="com.join.GoodsTradingProgram.entity.feelBack.FeelBack">
        <id property="id" column="id"/>
        <result property="uid" column="uid"/>
        <result property="gid" column="gid"/>
        <result property="status" column="status"/>
        <result property="str" column="str"/>
    </resultMap>

    <select id="listAll" resultType="com.join.GoodsTradingProgram.entity.feelBack.FeelBack">
    SELECT *
    FROM t_feelback
    </select>

    <select id="queryId" resultType="com.join.GoodsTradingProgram.entity.feelBack.FeelBack">
    SELECT *
    FROM t_feelback
    where uid=#{id}
    </select>


    <update id="updateFeelBack">
    UPDATE t_feelback
    SET status=#{feelback.status}
    WHERE id=#{feelback.id}
    </update>


    <delete id="deleteFeelBack">
    DELETE FROM t_feelback
    WHERE id=#{id}
    </delete>

    <insert id="addFeelBack" parameterType="com.join.GoodsTradingProgram.entity.feelBack.FeelBack">
    INSERT INTO t_feelback
    (uid,gid,str,date,status)
    VALUES (#{feelback.uid},#{feelback.gid},
    #{feelback.str},#{feelback.date},#{feelback.status})
    </insert>
</mapper>